views:
  - type: table
    name: Table
    filters:
      and:
        - file.inFolder("/Moves")
        - video_urls.isEmpty()
    order:
      - file.name
      - difficulty
      - level
      - type
      - created_date
      - file.ctime
      - video_urls
    sort:
      - property: level
        direction: ASC
      - property: video_urls
        direction: ASC
      - property: file.ctime
        direction: DESC
      - property: created_date
        direction: DESC
      - property: file.name
        direction: ASC
      - property: type
        direction: ASC
      - property: usage_frequency
        direction: ASC
      - property: move_confidence
        direction: DESC
    columnSize:
      file.name: 219
      note.level: 198
      note.vidoe_urls: 486
